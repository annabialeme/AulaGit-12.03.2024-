let numero = 60;

let limiteTabuada = 4;

if (limiteTabuada <= 0) {
    console.log("Insira um limite valido!");
}

if (limiteTabuada % 1 !== 0) {
    console.log("Insira um limite com numero inteiro.");
} else{
    for (let i = 1; i <= limiteTabuada; i++) {
        console.log(numero, "x", i, "= ", numero * i);
    }
}